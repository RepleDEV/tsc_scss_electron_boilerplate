{
    "name": "tsc_scss_electron_boilerplate",
    "version": "1.0.0",
    "description": "Typescript + Scss Electron Boilerplate.",
    "main": "./dist/core/index.js",
    "scripts": {
        "dev": "npm-run-all -p dev:renderer dev:electron",
        "dev:renderer": "webpack-dev-server --env.PORT=9102 --mode development",
        "dev:electron": "wait-on http-get://localhost:9102/ && tsc && cross-env ENV=dev electron .",
        "build": "npm-run-all -p build:renderer build:electron",
        "build:renderer": "webpack --mode production",
        "build:electron": "tsc",
        "prettier": "prettier --write .",
        "lint": "eslint"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/jquery": "^3.5.5",
        "@types/node": "^14.14.13",
        "@typescript-eslint/eslint-plugin": "^4.9.1",
        "@typescript-eslint/parser": "^4.9.1",
        "cross-env": "^7.0.3",
        "css-loader": "^5.0.1",
        "eslint": "^7.15.0",
        "html-webpack-plugin": "^4.5.0",
        "jquery": "^3.5.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1",
        "sass": "^1.30.0",
        "sass-loader": "^10.1.0",
        "style-loader": "^2.0.0",
        "ts-loader": "^8.0.12",
        "typescript": "^4.1.3",
        "wait-on": "^5.2.0",
        "webpack": "^4.44.2",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0"
    },
    "dependencies": {
        "electron": "^11.1.0"
    }
}
